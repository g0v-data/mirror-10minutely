    <script>
        var _IndexHome = {};
        $(document).ready(function () {
            //主視覺  
            $('.slider').bxSlider({
                touchEnabled: false,
                auto:true,
				speed:1500, //轉場速度
				pause:7000  //停留時間
            });
            $(".hidefirst").show();
            // 取得IndexHome 必要物件
            getIndexHome();
            //設定事件開設的資料
            setIframSrc();
        });

        //初始各類示警介紹
        function initialCapProfile() {
            //保留"最近上架示警"分類
            $('#CapProfileArea').not(':first').empty();
            var tabindex = 1000;
            var keymap = new Map();
            $.each(_IndexHome._listCapProfile, function (i, item) {
                
                //排除不重複組分類 
                if ($('a[name="LiveCagegoryGroup"]').text().indexOf(item.CategoryName) < 0) {
                    //組各類示警介紹的分類
                    var templateLiveCategory = $('#JsTemp_LiveCatagory').html();
                    tabindex += 1000;
                    keymap.set(item.CategoryName, tabindex);
                    templateLiveCategory = templateLiveCategory.replace("{{id}}", item.LiveCategoryID)
                                                                .replace("{{Name}}", item.CategoryName)
                                                                .replace("{{Name2}}", item.CategoryName)
                                                                .replace("{{TabIndex}}", tabindex);
                    
                    $('#LiveCategoryArea').html($('#LiveCategoryArea').html() +  "<li>" + templateLiveCategory + "</li>");
                    $(".indexPaneOverflowArea").append("<div id='Div_CapProfile"+tabindex+"' style='text-align:left;'></div>");
                }

                //組各類示警介紹的明細
                var templateCapProfile = $('#JsTemp_CapProfile').html();
                templateCapProfile = templateCapProfile.replace("{{AlertTypeID}}", item.Code)
                                                       .replace("{{AlertTypeName}}", item.AlertTypeName)
                                                       .replace("{{AlertTypeName2}}", item.AlertTypeName)
                                                       .replace("{{Introduction}}", item.Introduction)
                                                       .replace("{{IssueName}}", item.IssueName)
                                                       .replace("{{LiveCategoryID}}", item.LiveCategoryID)
                                                       .replace("{{ShowLatest}}", item.Flag)
                                                       .replace("{{LinkMore}}", '/platformInfo.aspx?SelectedCap=' + item.AlertTypeID)
                                                       .replace("{{TabIndex}}", keymap.get(item.CategoryName) + (i+1));

                $('#Div_CapProfile' + keymap.get(item.CategoryName)).append(templateCapProfile);
                if (item.Flag == "ShowLatest") {
                    var templateCapProfile1000 = $('#JsTemp_CapProfile').html();
                    templateCapProfile1000 = templateCapProfile1000.replace("{{AlertTypeID}}", item.Code)
                                                           .replace("{{AlertTypeName}}", item.AlertTypeName)
                                                           .replace("{{AlertTypeName2}}", item.AlertTypeName)
                                                           .replace("{{Introduction}}", item.Introduction)
                                                           .replace("{{IssueName}}", item.IssueName)
                                                           .replace("{{LiveCategoryID}}", item.LiveCategoryID)
                                                           .replace("{{ShowLatest}}", item.Flag)
                                                           .replace("{{LinkMore}}", '/platformInfo.aspx?SelectedCap=' + item.AlertTypeID)
                                                           .replace("{{TabIndex}}", 1000 + (i+1));

                    $('#Div_CapProfile1000').append(templateCapProfile1000);
                }
            });

            //綁定各類示警Click event
            bindLiveCagegory();

            //預設點選最新上架示警
            $('a[name="LiveCagegoryGroup"]:first').click();


            //綁定tab
            TabClick();
        };

        //綁定各類示警Click event
        function bindLiveCagegory() {
            $('a[name="LiveCagegoryGroup"]').click(function () {
                $('a[name="LiveCagegoryGroup"]').removeClass("selected");
                $(this).addClass("selected");
                $(this).fadeIn(300);

                var tabindex = $(this).attr("tabindex");

                //內容區塊先全隱藏
                for (var i = 1000; i < 10000; i = i + 1000) {
                    $("#Div_CapProfile" + i).hide();
                }
                $("#Div_CapProfile" + tabindex).show();
                //var selectedCategoryid = $(this).attr('categoryid');
                ////篩選最新上架示警
                //if ($(this).attr('livecategoryid') === 'latest') {
                //    $('div[name="alerts"]').each(function (index, value) {
                //        if ($(this).attr('showlatest') === 'ShowLatest') {
                //            $(this).show();
                //        } else {
                //            $(this).hide();
                //        }

                //    })
                //} else {
                //    $('div[name="alerts"]').each(function (index, value) {
                //        if ($(this).attr('livecategoryid') === selectedCategoryid) {
                //            $(this).show();
                //        } else {
                //            $(this).hide();
                //        }
                //    })
                //}

                //重新依照示警數量決定是否出現橫向卷軸
                $(".indexPaneOverflowArea").mCustomScrollbar("destroy");
                $(".indexPaneOverflowArea").mCustomScrollbar({
                    axis: "x",
                    theme: "dark-thin",
                    autoExpandScrollbar: true,
                    advanced: { autoExpandHorizontalScroll: true }
                });
            })
        }

        function FilterNews() {
            //卷軸
            //$(".newsoverview").mCustomScrollbar({
            //        theme: "dark-thin",
            //        autoExpandScrollbar: true,
            //        advanced: { autoExpandHorizontalScroll: true }
            //});

            if (typeof (_IndexHome._listAnnouncement) != "undefined" && _IndexHome._listAnnouncement.length > 0) {
                filterObj = _IndexHome._listAnnouncement;
                if (filterObj.length > 0) {
                    $.each(filterObj, function (index, obj) {
                        if (index < 5) { 
                        var template = $("#JsTemp_News").html();
                            $("#table_News").append(template.replace(/{{date}}/g, obj.date.substr(0,2) + "-" + obj.date.substr(2,2))
                                .replace(/{{Subject}}/g, obj.Subject)
                                .replace(/{{EditorContentPK}}/g, obj.EditorContentPK)
                                .replace(/{{year}}/g, obj.year+ "-")
                                );
                        }
                    });
                }
            }
        }

        function FilterAlertNow() {
            //卷軸
            $("ul.alertnowBox").mCustomScrollbar({
                    theme: "dark-thin",
                    autoExpandScrollbar: true,
                    advanced: { autoExpandHorizontalScroll: true }
            });

                if (_IndexHome._listCapImmediateImgCount !== null && _IndexHome._listCapImmediateImgCount.length > 0) {
                    filterObj = _IndexHome._listCapImmediateImgCount;
                    if (filterObj.length > 0) {

                        $(".alertnowBox").html("")
                        $.each(filterObj, function (index, obj) {
                            if (obj.Name == "ncdrSystemTest" || obj.Name == "1044") { 
                                //排除測試的
							}else{ 
								var template = $("#JsTemp_AlertNow").html();
								$(".alertnowBox").append(template.replace(/{{@AlertType}}/g, obj.Code)
									.replace(/{{@AlertName}}/g, obj.Name)
									.replace(/{{@Count}}/g, obj.Total)
									.replace(/{{@AlertTypeID}}/g, obj.Id)
								);
							}
                        });
                    }
                }
            }

            //ajax範例
            function Test() {
                var obj = {};
                obj.SUID = "BBBBB";
                $.ajax({
                    type: "POST",
                    url: "indexHome.aspx/Test",
                    dataType: "json",
                    async: true,
                    data: JSON.stringify(obj),
                    contentType: "application/json; charset=utf-8",
                    success: function (SuccessJson) {
                        alert(SuccessJson.d);
                    },
                    error: function (xhr, ajaxOtions, thrownError) {
                        $("body").append(xhr.status);
                        $("body").append(xhr.responseText);

                        alert(thrownError);
                    },
                    complete: function () {

                    }
                });
            }

            function getIndexHome() {
                $.ajax({
                    type: "POST",
                    url: "indexHome.aspx/GetIndexHome",
                    dataType: "json",
                    async: false,
                    contentType: "application/json; charset=utf-8",
                    success: function (SuccessJson) {
                        _IndexHome = SuccessJson.d;
                    },
                    error: function (xhr, ajaxOtions, thrownError) {
                        $("body").append(xhr.status);
                        $("body").append(xhr.responseText);

                        alert(thrownError);
                    },
                    complete: function () {

                        //初始各類示警介紹
                        initialCapProfile();

                        // 最新消息
                        FilterNews();

                        //即時示警
                        FilterAlertNow();
                    }
                });
            }

            function openAlert(url) {//示警開設點擊的彈跳視窗
                $.fancybox.open({ href: url, type: 'iframe' }, { maxWidth: 800, width: '90%' });
            }

        function setIframSrc() {//設定示警開設url以及標題
            if (document.getElementById('iframe_City') != undefined) {
                document.getElementById('iframe_City').src = "iframe_indexEventCity.aspx?EventStartTime=" + $("#hfdStarTime").val();
            }
            if (document.getElementById('iframe_Event') != undefined) {
                document.getElementById('iframe_Event').src = "iframe_indexEvent.aspx?EventStartTime=" + $("#hfdStarTime").val() + "&EventName=" + $("#hfdEventName").val();
            }
            $("#titleCity").text($("#hfdEventName").val() + " 來襲!!");
            $("#titleEvent").text($("#hfdEventName").val() + " 來襲!!");
        }


    </script>


