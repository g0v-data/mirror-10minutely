    <script type="text/javascript">
        $(document).ready(function () {


            //選單
            $(".phone_menu").click(function () {
                $(".phone_menu_close").show();
                $(".CAP_memuBar").fadeIn(300);
                //$("body").css({'overflow':'hidden'});  必殺技 沒事不要用

            });

            $(".phone_menu_close").click(function () {
                $(".phone_menu").show();
                $(".CAP_memuBar").fadeOut(300);
                //$("body").css({'overflow':'auto'}); 必殺技 沒事不要用
            });

            //修正螢幕被拉大時menu不顯示的bug，使用$(window).width()取得目前遊覽器視窗寬度。
            $(window).resize(function () {
                wdth = $(window).width();
                if (wdth > 760) { $(".CAP_memuBar").show(); } else if (wdth < 759) { $(".CAP_memuBar").hide(); }
            });

            //選單下拉
            $(".CAP_memuBar  li").mouseover(function () {
                $(this).find(".CAP_menu_sub").show();
            }).mouseleave(function () {
                $(this).find(".CAP_menu_sub").hide();
            });
            //用tab鍵開下拉
            $(".CAP_memuBar ul  li >a").focus(function () {
                $(".CAP_menu_sub").hide();
                $(this).siblings(".CAP_menu_sub").show();
            });
            $(".accesskeyStyleCon").focus(function () {
                $(".CAP_menu_sub").hide();
            });


            //文字大小切換	
            $('.txtchange_S').click(function () {
                $('.fontchange .txtchange').removeClass("sel");
                $(this).addClass("sel");
                $("body").css({ 'fontSize': '70%' });
            });
            $('.txtchange_M').click(function () {
                $('.fontchange .txtchange').removeClass("sel");
                $(this).addClass("sel");
                $("body").css({ 'fontSize': '100%' });
            });
            $('.txtchange_L').click(function () {
                $('.fontchange .txtchange').removeClass("sel");
                $(this).addClass("sel");
                $("body").css({ 'fontSize': '200%' });
            });	

            //  fancybox
            $(".fancyIn").fancybox();
            $(".fancyiframePage").fancybox({
                padding: '0',
                //margin: '0 , 20',
                type: 'iframe',
                autoSize: false,
                width: '95%',
                maxWidth:'650px',
                height: 'auto',
                minHeight: '400px'
                // helpers: { overlay: { closeClick: false } }
            });
            $(".fancyiframePageMovie").fancybox({
                padding: '0',
                //margin: '0 , 20',
                type: 'iframe',
                autoSize: false,
                width: '95%',
                maxWidth:'580px',
                height: 'auto',
                minHeight: '355px',
                scrolling: 'no'
                // helpers: { overlay: { closeClick: false } }
            });



            $("#radioType_1").on("click", function () {
                 setShow();
            });
            $("#radioType_0").on("click", function () {
                 setDisable();
            });           

            //依照當前頁面來決定是否要顯示示警資料內容問題管理radio  -star
           let PageVal =  $("#PageMangerType").val();
            if (PageVal == "1") {
                setDisable();
            }
            else {
                setShow();
            }
            
            //依照當前頁面來決定是否要顯示示警資料內容問題管理radio  -end


            setDisable();

            //註冊function -star
            $("#dropType").change(function () { setCategoryVal(); });
            $("#ddlCategory").change(saveddlCategoryVal());
            //註冊function -end



            //處理超連結沒有給tabindex的
            $("a , input , select").each(function () {
                if ($(this).attr("tabindex") == undefined) {
                    $(this).attr("tabindex", 500);
                }
            })



        });

    </script>



