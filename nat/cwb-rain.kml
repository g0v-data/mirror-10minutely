        function ShowProgressBar() {
            displayProgress();
            displayMaskFrame();
        }


        function HideProgressBar() {
            var progress = $('#dvhide');
            var maskFrame = $("#dvmask");
            progress.hide();
            maskFrame.hide();
        }

        function displayProgress() {
            var w = $(document).width();
            var h = $(window).height();
            var progress = $('#dvhide');
            progress.css({ "z-index": 999999, "top": (h / 2) - (progress.height() / 2), "left": (w / 2) - (progress.width() / 2) });
            progress.show();

        }

        function displayMaskFrame() {
            var w = $(window).width();
            var h = $(document).height();
            var maskFrame = $("#dvmask");
            maskFrame.css({ "z-index": 999998, "opacity": 0.7, "width": w, "height": h });
            maskFrame.show();

        }


        //設定示警名稱下拉式選單是否出現-star
        function setDisable() {
            $("#h4ID").hide();
            $("#pID").hide();
            $("#h4ID2").hide();
            $("#pID2").hide();
        }

        function setShow() {
            $("#h4ID").show();
            $("#pID").show();
            $("#h4ID2").show();
            $("#pID2").show();
        }
        //設定示警名稱下拉式選單是否出現-end

        //依照使用者選擇示警類別來更新下拉式選單的內容
        function setCategoryVal() {
           var obj = {};
            obj.LiveCategoryId = $("#dropType").val();
            $("#hfdTblcapreciveInfoSuid").val($("#dropType").val());
           $.ajax({
                type: "GET",
                url: "webService/ForMasterPage.ashx?LiveCategoryId="+obj.LiveCategoryId,           
                async: false,                
                contentType: "application/json; charset=utf-8",
                dataType: 'json',
                success: function (SuccessJson) {                    
                    $("#ddlCategory").html("");
                    $.each(SuccessJson, function (index, obj) {
                        let opt = document.createElement('option');
                        opt.innerText = obj.Name+"("+obj.IssueName+")";
                        opt.value = obj.AlertTypeId;
                        $("#ddlCategory").append(opt);
                    });
                    BindEvents();
                    saveddlCategoryVal();
                 },
                 error: function (xhr, ajaxOtions, thrownError) {
                     $("body").append(xhr.status);
                     $("body").append(xhr.responseText);

                     alert(thrownError);
                 },
                 complete: function () {
                    
                 }
             });
        }


        function saveddlCategoryVal() {
            let selectVal = $("#ddlCategory").val();
            $("#hfdAlertType").val(selectVal); 
        }

        function BindEvents() {
            $("#ddlCategory").change(function () { saveddlCategoryVal();});
        }

        function setMailText(pageName) {
            $("#setMailText").text(pageName+"意見回饋");
        }
    </script>



    

